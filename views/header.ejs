<!DOCTYPE html>
<html lang="en" dir="ltr">

<head>
  <meta charset="utf-8">
  <title>Bank Management System</title>


  <script>
    function startTime() {
      var today = new Date();
      var h = today.getHours();
      var ampm = h >= 12 ? ' PM' : ' AM';
      h = h % 12;
      h = h ? h : 12;
      var m = today.getMinutes();
      var s = today.getSeconds();
      m = checkTime(m);
      s = checkTime(s);
      document.getElementById('txt').innerHTML = '<h6 style="font-size: 1.2rem;">Time: ' +
        h + ":" + m + ":" + s + ampm + '</h6>';
      var t = setTimeout(startTime, 500);
    }
    function checkTime(i) {
      if (i < 10) { i = "0" + i };  // add zero in front of numbers < 10
      return i;
    }
  </script>


  <!-- font-awesome -->
  <script src="https://kit.fontawesome.com/ee8e198b74.js" crossorigin="anonymous" SameSite="None"></script>

